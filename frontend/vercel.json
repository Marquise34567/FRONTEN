{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(?!site\.webmanifest|favicon-.*|robots\.txt|_next/.*|assets/.*|favicon\.ico)(.*)",
      "destination": "/index.html"
    }
  ]
}
